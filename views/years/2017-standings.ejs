<!doctype html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body id="page-top" data-spy="scroll" data-target=".navbar-fixed-top">
<% include ../partials/header %>

<!-- Intro Header -->
<header class="intro">
    <div class="intro-body">
        <div class="container">
            <div class="row">
                <div class="col-md-8 col-md-offset-2">
                    <% if (typeof user == 'undefined') { %>
                    <p>Login with:</p>
                    <p><a href="/auth/strava"><img src="./btn_strava_connectwith_orange.png"></a></p>
                    <% } else { %>
                    <h1 class="brand-heading">LeFernando 2017</h1>

                    <h2>Standings</h2>
                    <p>Solo
                        <a href="#solo" class="btn btn-circle page-scroll">
                            <i class="fa fa-angle-double-down animated"></i>
                        </a></p>
                    <p>Team
                        <a href="#team" class="btn btn-circle page-scroll">
                            <i class="fa fa-angle-double-down animated"></i>
                        </a></p>
                    <p>Climb
                        <a href="#climb" class="btn btn-circle page-scroll">
                            <i class="fa fa-angle-double-down animated"></i>
                        </a></p>
                    <p>Sprint
                        <a href="#sprint" class="btn btn-circle page-scroll">
                            <i class="fa fa-angle-double-down animated"></i>
                        </a></p>
                    <% } %>
                </div>
            </div>
        </div>
    </div>
</header>

<section id="solo" class="container content-section text-center">
    <div class="row">
        <div class="col-lg-8 col-lg-offset-2">
            <iframe width="100%" height="620px" src="https://docs.google.com/spreadsheets/d/1HTMT7oZgxWKd7rF-YsDYdleYYuXzRHqGbRTi5295Pw4/pubhtml?gid=0&amp;single=true&amp;widget=true&amp;headers=false"></iframe>
        </div>

        </div>
</section>

<section id="team" class="container content-section text-center">
    <div class="row">
        <div class="col-lg-12 col-lg-offset-0">
            <iframe width="100%" height="620px" src="https://docs.google.com/spreadsheets/d/1HTMT7oZgxWKd7rF-YsDYdleYYuXzRHqGbRTi5295Pw4/pubhtml?gid=7&amp;single=true&amp;widget=true&amp;headers=false"></iframe>
        </div>

    </div>
</section>

<section id="climb" class="container content-section text-center">
    <div class="row">
        <div class="col-lg-8 col-lg-offset-2">
            <iframe width="100%" height="1100px" src="https://docs.google.com/spreadsheets/d/1HTMT7oZgxWKd7rF-YsDYdleYYuXzRHqGbRTi5295Pw4/pubhtml?gid=8&amp;single=true&amp;widget=true&amp;headers=false"></iframe>
        </div>

    </div>
</section>

<section id="sprint" class="container content-section text-center">
    <div class="row">
        <div class="col-lg-8 col-lg-offset-2">
            <iframe width="100%" height="1100px" src="https://docs.google.com/spreadsheets/d/1HTMT7oZgxWKd7rF-YsDYdleYYuXzRHqGbRTi5295Pw4/pubhtml?gid=9&amp;single=true&amp;widget=true&amp;headers=false"></iframe>
        </div>

    </div>
</section>


<footer>
    <% include ../partials/footer %>
</footer>
</body>
</html>
