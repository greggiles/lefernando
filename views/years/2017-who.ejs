<!-- Intro Header -->
<header class="intro">
    <div class="intro-body">
        <div class="container">
            <div class="row">
                <div class="col-md-8 col-md-offset-2">
                    <% if (typeof user == 'undefined') { %>
                    <p>Login with:</p>
                    <p><a href="/auth/strava"><img src="./btn_strava_connectwith_orange.png"></a></p>
                    <% } else { %>
                    <h1 class="brand-heading">LeFernando 2017</h1>
                    <p>
                        See <a href="/lf2017">Stage Details<a/>
                    </p>

                    <h2>Rider List Below</h2>
                    <a href="#riders" class="btn btn-circle page-scroll">
                        <i class="fa fa-angle-double-down animated"></i>
                    </a>
                    <% } %>
                </div>
            </div>
        </div>
    </div>
</header>

<section id="riders" class="container content-section text-center">
